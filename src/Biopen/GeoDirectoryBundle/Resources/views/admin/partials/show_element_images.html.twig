{% extends "@BiopenAdmin/core_custom/show_choice_base.html.twig" %}

{% block display_value %}
    {% for key, item in value %}
     {% if item.imageName %}
      <img src="{{ asset('uploads/images/elements/' ~ item.imageName) }}" class="thumbnail"/>
     {% endif %}
    {% endfor %}

    <style>
      .thumbnail {     
        width: 100px;
        display: inline-block; 
        margin-right: 10px;
      }
    </style>
{% endblock %}
